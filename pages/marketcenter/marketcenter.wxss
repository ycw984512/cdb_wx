.top_wrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background: #fff;
  z-index: 99;

}

.tab_bar {
  margin: 0rpx 20rpx 20rpx 20rpx;
}

.border_middle {
  box-sizing: border-box;
  margin: 0 auto;
  height: 2rpx;
}

.share-btn {
  width: 150rpx;
  height: 50rpx;
  border: 1px solid #1b82d1;
  line-height: 50rpx;
  text-align: center;
  border-radius: 50rpx;
  margin-left: 20rpx;
  color: #1b82d1;
  padding: 0rpx;
  font-size: 26rpx;
  box-sizing: content-box;
  margin-top: 20rpx;
}

.button-hover {
  background: none;
}

.index_introduction {
  font-size: 24rpx;
  text-indent: 2em;
  line-height: 45rpx;
  color: #999;
}

.index_image image {
  width: 100%;
  margin-top: 15rpx;
  height: auto;
}

.CouponList {
  display: flex;
  height: 176rpx;
  padding: 10rpx 0rpx;
  position: relative;
}

.CouponList_img {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.CouponList_img image {
  width: 150rpx;
  height: 150rpx;
}

.CouponList_name {
  display: flex;
  flex-direction: column;
  margin-left: 24rpx;
  justify-content: space-around;
}

.CouponList_rate {
  display: flex;
  flex-direction: column;
  margin-left: 37rpx;
  justify-content: flex-end;
  position: absolute;
  right: 182rpx;
  bottom: 2rpx;
}

.CouponList_money {
  display: flex;
  flex-direction: column;
  position: absolute;
  margin-left: 24rpx;
  justify-content: flex-end;
  right: -6rpx;
  bottom: 10rpx;
}

.CouponList_money view {
  margin-bottom: 10rpx;
}

.border_bottom {
  height: 1px;
  background: #999;
}

.rate {
  display: flex;
  justify-content: flex-end;
  flex-direction: column;
}

.extension_rate {
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(255, 161, 27, 1);
  margin-top: 20rpx;
  margin-right: 10rpx;
}

.lock_rate {
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(71, 104, 243, 1);
  margin-top: 20rpx;
}
.bottom_img {
  display: inline-block;
  position: absolute;
  top:39rpx;
  right: 15rpx;
}
.bottom_img image{
  width: 125rpx;
  height:125rpx;
}


.lock_btn, .lock_btn1 {
  display: flex;
}

.lock_btn view, .lock_btn1 view {
  width: 150rpx;
  height: 50rpx;
  border: 1px solid #999;
  text-align: center;
  line-height: 50rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  margin-top: 20rpx;
  border-radius: 40rpx;
}

.lock_btn view:nth-child(1) {
  color: rgba(153, 153, 153, 1);
}

.lock_btn view:nth-child(2) {
  color: #1b82d1;
  margin-left: 20rpx;
  border: 1px solid #1b82d1;
}

.lock_btn1 view:nth-child(1) {
  color: rgba(153, 153, 153, 1);
}

.lock_btn1 view:nth-child(2) {
  color: rgba(153, 153, 153, 1);
  margin-left: 20rpx;
}

.lock_btn1 view:nth-child(3) {
  color: #1b82d1;
  margin-left: 20rpx;
  border: 1px solid #1b82d1;
}

.market_wrap {
  width: 94%;
  margin: 30rpx auto;
  display: flex;
  justify-content: flex-start;
  padding-bottom: 20rpx;
  border-bottom: 1px solid #ccc;
  position: relative;
}

.market_wrap image {
  width: 80rpx;
  height: 80rpx;
}

.coupon {
  font-size: 26rpx;
  margin-left: 32rpx;
  color: rgba(153, 153, 153, 1);
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.market_title {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-left: 20rpx;
}

.market_title view {
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.market_title text {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}

