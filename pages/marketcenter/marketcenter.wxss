}

.CouponList_money view {
  margin-bottom: -1rpx;
}

.border_bottom {
  height: 1px;
  background: #ccc;
}

.wrap {
  background: white;
  margin-bottom: 20rpx;
  padding-bottom: 20rpx;
  margin-top: 20rpx;
}

page {
  background: #eee;
  min-height: 100%;
}

.new_coupon {
  width: 96%;
  height: 280rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  position: relative;
  margin-bottom: 20rpx;
}

new_coupon:nth-child(1) {
  margin-top: 0rpx;
}

.top_left {
  position: absolute;
  top: 0;
  left: 0;
  width: 66%;
  height: 80%;
  display: flex;
}

.top_left .top_left_img {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 43%;
  padding-left: 10rpx;
}

.top_left .top_left_img image {
  width: 200rpx;
  height: 200rpx;
  border-radius:20rpx;

}

.top_left .top_left_name {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  height: 207rpx;
  margin-top: 5rpx;
  width: 60%;
}

.top_left .top_left_name_goods {
  font-size: 30rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 100%;
  padding: 0 15rpx;
  box-sizing: border-box;
  font-weight: 700;
}

.top_left .top_left_name_store {
  font-size: 26rpx;
  display: flex;
  width: 100%;
  margin-bottom: 42rpx;
}

.top_left .top_left_name_store view:nth-child(1) {
  max-width: 64%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  text-align: center;
}

.top_left .top_left_name_store view:nth-child(2) {
  position: absolute;
  top: 0rpx;
  right: -229rpx;
  margin-top: 10rpx;
}

.top_left .top_left_name_rate {
  font-size: 16rpx;
  display: flex;
  color: #e75252;
  width: 100%;
}

.top_left .top_left_name_rate view:nth-child(1) {
  padding: 6rpx;
  border: 1px solid #e75252;
  border-radius: 12rpx;
  margin-left: 12rpx;
}

.top_left .top_left_name_rate view:nth-child(2) {
  padding: 6rpx;
  margin-left: 11rpx;
  border: 1px solid #e75252;
  border-radius: 12rpx;
}

.top_right {
  position: absolute;
  top: 0;
  right: 0;
  width: 34%;
  height: 80%;
}

.top_right .top_right_wrap {
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  margin-top: 30rpx;
}

.top_right .top_right_wrap view:nth-child(1) {
  font-size: 30rpx;
  color: #e75252;
  margin-top: 20rpx;
}

.top_right .top_right_wrap  view:nth-child(2) {
  font-size: 56rpx;
  color: #e75252;
  font-weight: 700;
}

.top_right  .top_right_wrap view:nth-child(2) text {
  font-size: 30rpx;
  color: #e75252;
  font-weight: 300;
}

.top_right .top_right_wrap  view:nth-child(3) {
  font-size: 30rpx;
  color: #e75252;
  margin-bottom: 33rpx;
}

.bottom_left {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 66%;
  height: 20%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.bottom_right {
  position: absolute;
  bottom: 0;
  right: 0;
  width: 34%;
  height: 20%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.bottom_left  view {
  font-size: 24rpx;
  margin-right: 50rpx;
}

.bottom_right view {
  font-size: 24rpx;
}
