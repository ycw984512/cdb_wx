.CouponList_wrap {
  margin: 10rpx auto 30rpx;
  width: 96%;
}

.CouponList_wrap image {
  width: 100%;

}

.CouponList {
  display: flex;
  height: 176rpx;
  padding: 10rpx 0rpx;
  position: relative;
}

.CouponList_img {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.CouponList_img image {
  width: 150rpx;
  height: 150rpx;
}

.CouponList_name {
  display: flex;
  flex-direction: column;
  margin-left: 24rpx;
  justify-content: space-around;
}

.CouponList_rate {
  display: flex;
  flex-direction: column;
  margin-left: 24rpx;
  justify-content: flex-end;
}

.CouponList_money {
  display: flex;
  flex-direction: column;
  position: absolute;
  margin-left: 24rpx;
  justify-content: flex-end;
  right: 10rpx;
  bottom: 10rpx;
}

.CouponList_money view {
  margin-bottom: 10rpx;
}

.border_bottom {
  height: 1px;
  background: #999;
}

.rate {
  display: flex;
  justify-content: flex-end;
}

.extension_rate {
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(255, 161, 27, 1);
  margin-top: 20rpx;
  margin-right: 50rpx;
}

.lock_rate {
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: #1b82d1;
  margin-top: 20rpx;
}

.use_instructions {
  width: 100%;
  padding: 30rpx 0 30rpx 0;
}

.user_title {
  margin: 0 0 0rpx 20rpx;
  width: 100%;
  font-size: 28rpx;
  font-weight: 500;
}

.user_point {
  margin: 20rpx 0 0 50rpx;
  width: 100%;
  font-size: 28rpx;
  color: #666;
}

.user_point text {

  width: 100%;
  display: inline-block;
  word-break: break-all;

}

.validity, .validity_data {
  margin-left: 50rpx;
  margin-top: 30rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-bottom: 50rpx;
}

.validity {
  margin-left: 20rpx;
}

.validity_data {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}

.Coupon {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-top: 60rpx;
  position: relative;
  padding-bottom: 50rpx;
  margin-bottom: 162rpx;
}

.Coupon_img {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
}

.Coupon_img image {
  width: 156rpx;
  height: 156rpx;
}

.Coupon_name {
  display: flex;
  flex-direction: column;
  margin-left: 24rpx;
  justify-content: flex-start;
}

.Coupon_money {
  display: flex;
  justify-content: flex-start;
  font-size: 24rpx;
}

.Coupon_money view {
  width: 140rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  box-shadow: rgba(0, 0, 0, 0.2) solid 1px;
  border-radius: 10rpx;
  font-size: 24rpx;
}

.bookMark image {
  width: 23rpx;
  height: 23rpx;
}

.bookMark_icon image {
  width: 23rpx;
  height: 23rpx;
}

.bookMark {
  background: #fe012b;
}

.bookMark_title {
  font-size: 24rpx;
  color: #fff;
}

.btn {
  width: 100%;
  height: 160rpx;
  border-top: 1px solid #ccc;
  box-shadow: #eee solid 1px;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0rpx;
  background: #fff;
}

.btn_download {
  width: 690rpx;
  height: 88rpx;
  font-size: 32rpx;
  color: #fff;
  line-height: 88rpx;
  text-align: center;
  background: #1b82d1;
  border-radius: 60rpx;
}

.kcrzxy {
  width: 70%;
  left: 50%;
  top: 25%;
  position: fixed;
  transform: translate(-50%, 0);
  background: #fff;
  border-radius: 10px;
  z-index: 999;
}

.kcrzxyhd {
  text-align: center;
  font-size: 30rpx;
  background: #1b82d1;
  height: 70rpx;
  line-height: 70rpx;
  color: white;
  border-top-left-radius: 10px;
  border-top-right-radius: 10px;
}

.kcrzxybd {
  font-size: 28rpx;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}

.queren, .quxiao {
  height: 70rpx;
  width: 30%;
  font-size: 30rpx;
  line-height: 70rpx;
  text-align: center;
  color: white;
  background: #1b82d1;
  border-radius: 10rpx;
  margin: 5px 0;
}

wx-button:active {
  color: rgba(255, 255, 255, 0.6);
  opacity: 0.6;
}

.zhezhao {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 998;
}

.holder {
  font-size: 28rpx;
  text-align: center;
}

.settingItem {
  display: flex;
  width: 92%;
  justify-content: flex-start;
  background: white;
  border-bottom: #dedede solid 1px;
  padding: 22rpx 0rpx 22rpx 0rpx;
  align-items: center;
  position: relative;
  margin: 0 auto;
}

.settingItem .caption {
  margin-left: 20rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.settingItem .daohang {
 font-size:30rpx;
padding:5rpx 15rpx;
background:#e60000;
color:#fff;
border-radius:10rpx;
position: absolute;
right: 20rpx;
}




.settingItem .icon {
  margin-left: 20rpx;
}

.settingItem .icon_img {
  width: 28rpx;
  position: absolute;
  right: 20rpx;
}

.image_wrap {
  width: 92%;
  margin: 20rpx auto;
}

.index_image image {
  width: 100%;
}

.backindex {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  position: fixed;
  bottom: 12%;
  right: 1%;
  z-index: 900;
  font-size: 24rpx;
  color: #fff;
  padding: 20rpx;
  text-align: center;
  letter-spacing: 5rpx;
  line-height: 30rpx;

}

.backindex image {
  width: 100rpx;
  height: 100rpx;
}
.user_point1{
  margin:0rpx 0 0 50rpx;
font-size:28rpx;
color:#666;

}